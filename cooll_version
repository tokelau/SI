#include <stdio.h> // Стандартная библиотека для ввода-вывода
#include <dirent.h> // Библиотека работы с директориями
#include <string.h>
#include <math.h>

main()
{

    float  csr=0;
    float y[32];
    int i;
        for(i=0; i<32; i++)
        {
            y[i]=(float)2;
        }
    for (i=0; i<32; i++)
    {
        csr=csr+y[i];
    }
    csr/=(float)32;
    for (i=0; i<=31; i++)
        y[i]=(y[i]-csr)*(y[i]-csr);
    for (i=1; i<=31; i++)
        y[i]=y[i-1]+y[i];
    y[31]/=(float)32;
    printf("%f\n", sqrt(y[31]));
}
